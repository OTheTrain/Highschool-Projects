package introjavalanguagefeatures2testing;

/**
 *
 * @author s.oaorsini
 */
public class Introjavalanguagefeatures2testing {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
        // TODO code application logic here
        for(int i = 1; i <= 6; i++)
        {
            for (int k = 1; k <= 6; k++)
                if (k==i)
                    System.out.print(2 * k);
                else
                    System.out.print("-");
            System.out.println();
            
        }
        
    }
    
}
